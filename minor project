   "source": [
    "import numpy as np\n",
    "img_size=28\n",
    "x_trainer=np.array(x_train).reshape(-1,img_size,img_size,1)\n",
    "x_tester=np.array(x_test).reshape(-1,img_size,img_size,1)\n",
    "print(\"Training samples dimension\",x_trainer.shape)\n",
    "print(\"Testing Samples dimension\",x_tester.shape)"
   ]
